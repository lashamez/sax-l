<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">

    <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Forgot</servlet-name>
        <jsp-file>/forgot.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Forgot</servlet-name>
        <url-pattern>/forgot</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UsersList</servlet-name>
        <jsp-file>/users-list.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsersList</servlet-name>
        <url-pattern>/users-list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>saxl</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>saxl</servlet-name>
        <url-pattern>/server-logs</url-pattern>
        <url-pattern>/admin</url-pattern>
        <url-pattern>/inbox</url-pattern>
        <url-pattern>/contact</url-pattern>
        <url-pattern>/statistics</url-pattern>
        <url-pattern>/activities</url-pattern>

        <url-pattern>*.html</url-pattern>
    </servlet-mapping>
</web-app>
